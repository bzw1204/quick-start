{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "moduleResolution": "Bundler",
    "paths": {
      "@/*": [
        "src-electron/*"
      ]
    },
    "types": [
      "electron-vite/node"
    ]
  },
  "include": [
    "src-electron/**/*",
    "@types/*.d.ts",
    "electron.vite.config.ts"
  ]
}
