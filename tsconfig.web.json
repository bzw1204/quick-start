{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "compilerOptions": {
    "composite": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "types": [
      "naive-ui/volar"
    ],
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": [
    "electron/share/*",
    "types/*.d.ts",
    "src/**/*"
  ]
}
